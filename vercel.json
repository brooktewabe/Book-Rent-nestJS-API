{
    "version":2,
    "env":{
        "DB_HOST":"localhost",
        "DB_PORT":"5432",
        "DB_USERNAME":"postgres",
        "DB_PASSWORD":"1234",
        "DB_NAME":"capstone",
        "PORT":"5000"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods":[
                "GET",
                "POST",
                "PATCH",
                "PUT",
                "DELETE"
            ]
        }
    ]
}